@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lude: <https://linkeduniversities.es/vocab/> .
@prefix mod: <https://w3id.org/mod#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dcterms:created a owl:AnnotationProperty .

dcterms:description a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

vann:preferredNamespaceUri a owl:AnnotationProperty .

rdfs:label a owl:DatatypeProperty ;
    rdfs:label "label" .

lude:aboutDegree a owl:ObjectProperty ;
    rdfs:label "about degree" ;
    rdfs:domain lude:ProgramOffering ;
    rdfs:range lude:Degree .

lude:academicYear a owl:DatatypeProperty ;
    rdfs:label "academic year" ;
    rdfs:domain lude:ProgramOffering .

lude:adjudicationRound a owl:DatatypeProperty ;
    rdfs:label "adjudication round" ;
    rdfs:domain lude:AdmissionCutoff .

lude:admissionPathway a owl:DatatypeProperty ;
    rdfs:label "admission pathway" ;
    rdfs:domain lude:AdmissionCutoff .

lude:branch a owl:DatatypeProperty ;
    rdfs:label "branch" ;
    rdfs:domain lude:Degree ;
    rdfs:range xsd:string .

lude:deliveredAt a owl:ObjectProperty ;
    rdfs:label "delivered at" ;
    rdfs:domain lude:ProgramOffering ;
    rdfs:range lude:Faculty .

lude:ectsCredits a owl:DatatypeProperty ;
    rdfs:label "ects credits" ;
    rdfs:domain lude:Degree .

lude:hasCutoff a owl:ObjectProperty ;
    rdfs:label "has cutoff" ;
    rdfs:domain lude:ProgramOffering ;
    rdfs:range lude:AdmissionCutoff .

lude:hasFaculty a owl:ObjectProperty ;
    rdfs:label "has faculty" ;
    rdfs:domain lude:University ;
    rdfs:range lude:Faculty .

lude:locatedIn a owl:ObjectProperty ;
    rdfs:label "located in" ;
    rdfs:domain lude:ProgramOffering ;
    rdfs:range lude:Municipality .

lude:offeredBy a owl:ObjectProperty ;
    rdfs:label "offered by" ;
    rdfs:domain lude:ProgramOffering ;
    rdfs:range lude:University .

lude:sameAsWikidata a owl:ObjectProperty ;
    rdfs:label "same as wikidata" ;
    rdfs:domain lude:Degree,
        lude:Municipality,
        lude:University ;
    rdfs:range rdfs:Resource .

lude:score a owl:DatatypeProperty ;
    rdfs:label "score" ;
    rdfs:domain lude:AdmissionCutoff .

lude:seatsAvailable a owl:DatatypeProperty ;
    rdfs:label "seats available" ;
    rdfs:domain lude:ProgramOffering .

lude:teachingModality a owl:DatatypeProperty ;
    rdfs:label "teaching modality" ;
    rdfs:domain lude:ProgramOffering .

lude:updateDate a owl:DatatypeProperty ;
    rdfs:label "update date" ;
    rdfs:domain lude:AdmissionCutoff .

rdfs:Resource a owl:Class ;
    rdfs:label "Resource" .

lude: a owl:Ontology ;
    dcterms:created "2025-10-06"^^xsd:date ;
    dcterms:description "Vocabulary for universities, faculties, degrees, program offerings and cut-offs across Spanish regions."@en ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcterms:title "Linked Universities Data Explorer Ontology"@en ;
    vann:preferredNamespacePrefix "lude" ;
    vann:preferredNamespaceUri lude: ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

lude:Faculty a owl:Class ;
    rdfs:label "Faculty" .

lude:Municipality a owl:Class ;
    rdfs:label "Municipality" .

lude:University a owl:Class ;
    rdfs:label "University" .

lude:Degree a owl:Class ;
    rdfs:label "Degree" .

lude:AdmissionCutoff a owl:Class ;
    rdfs:label "Admission Cutoff" .

lude:ProgramOffering a owl:Class ;
    rdfs:label "Program Offering" .

